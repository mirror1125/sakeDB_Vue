ate>\n\n<script>\nimport firebase from 'firebase'\n\nexport default {\n  name: 'Signup',\n  data: () => ({\n    showPassword: false,\n    showConfPass: false,\n    username: '',\n    password: '',\n    conf_pass: '',\n  }),\n  methods: {\n    signUp: function () {\n      firebase.auth().createUserWithEmailAndPassword(this.username, this.password)\n        .then(res => {\n          alert('ユーザ登録完了 : ' + res.user.email)\n          this.$router.push('/signin')\n        })\n        .catch(error => {\n          alert(error.message)\n        })\n    }\n  }\n}\n</script>"]}]}