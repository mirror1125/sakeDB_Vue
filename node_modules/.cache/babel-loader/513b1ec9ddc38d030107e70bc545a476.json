  </v-card-title>\n      <v-card-text>\n        <v-form>\n          <v-text-field prepend-icon=\"mdi-account-circle\" label=\"メールアドレス\" v-model=\"username\" />\n          <v-text-field v-bind:type=\"showPassword ? 'text' : 'password'\" prepend-icon=\"mdi-lock\" v-bind:append-icon=\"showPassword ? 'mdi-eye' : 'mdi-eye-off'\" label=\"パスワード\" v-model=\"password\" @click:append=\"showPassword = !showPassword\" />\n          <v-text-field v-bind:type=\"showConfPass ? 'text' : 'password'\" prepend-icon=\"mdi-lock\" v-bind:append-icon=\"showConfPass ? 'mdi-eye' : 'mdi-eye-off'\"   label=\"パスワード(確認用)\" v-model=\"conf_pass\" @click:append=\"showConfPass = !showConfPass\" />\n          <v-card-actions>\n            <v-btn @click=\"signUp\" class=\"info\">登録</v-btn>\n          </v-card-actions>\n        </v-form>\n      </v-card-text>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport firebase from 'firebase'\n\nexport default {\n  name: 'Signup',\n  data: () => ({\n    showPassword: false,\n    showConfPass: false,\n    username: '',\n    password: '',\n    conf_pass: '',\n  }),\n  methods: {\n    signUp: function () {\n      firebase.auth().createUserWithEmailAndPassword(this.username, this.password)\n        .then(res => {\n          alert('ユーザ登録完了 : ' + res.user.email)\n          this.$router.push('/signin')\n        })\n        .catch(error => {\n          alert(error.message)\n        })\n    }\n  }\n}\n</script>"],"sourceRoot":"src/components"}]}