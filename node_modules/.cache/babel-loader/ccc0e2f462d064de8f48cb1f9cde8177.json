-card width=\"400px\" class=\"mx-auto mt-5\">\n      <v-card-title>\n        <h1 class=\"display-1\">新規登録</h1>\n      </v-card-title>\n      <v-card-text>\n        <v-form>\n          <v-text-field prepend-icon=\"mdi-account-circle\" label=\"メールアドレス\" v-model.trim.lazy=\"$v.username.$model\" />\n          <div class=\"error\" v-if=\"!$v.username.required\">入力してください</div>\n          <v-text-field v-bind:type=\"showPassword ? 'text' : 'password'\" prepend-icon=\"mdi-lock\" v-bind:append-icon=\"showPassword ? 'mdi-eye' : 'mdi-eye-off'\" label=\"パスワード\" v-model.trim=\"$v.password.$model\" @click:append=\"showPassword = !showPassword\" />\n          <div class=\"error\" v-if=\"!$v.password.required\">入力してください</div>\n          <div class=\"error\" v-if=\"!$v.password.minLength\"> {{$v.password.$params.minLength.min}}文字以上にしてください</div>\n          <v-text-field v-bind:type=\"showConfPass ? 'text' : 'password'\" prepend-icon=\"mdi-lock\" v-bind:append-icon=\"showConfPass ? 'mdi-eye' : 'mdi-eye-off'\"   label=\"パスワード(確認用)\" v-model.trim=\"$v.conf_pass.$model\" @click:append=\"showConfPass = !showConfPass\" />\n          <div class=\"error\" v-if=\"!$v.conf_pass.required\">入力してください</div>\n          <div class=\"error\" v-if=\"!$v.conf_pass.minLength\"> {{$v.password.$params.minLength.min}}文字以上にしてください</div>\n          <v-card-actions>\n            <v-btn @click=\"signUp\" class=\"info\">登録</v-btn>\n          </v-card-actions>\n        </v-form>\n      </v-card-text>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport firebase from 'firebase'\nimport { require, minLength } from 'vuelidate/lib/validators'\n\nexport default {\n  name: 'Signup',\n  data: () => ({\n    showPassword: false,\n    showConfPass: false,\n    username: '',\n    password: '',\n    conf_pass: '',\n  }),\n  validations: {\n    username: {\n      require,\n    },\n    password: {\n      require,\n      minLength: minLength(8)\n    },\n    conf_pass: {\n      require,\n      minLength: minLength(8)\n    }\n  },\n  methods: {\n    signUp: function () {\n      firebase.auth().createUserWithEmailAndPassword(this.username, this.password)\n        .then(res => {\n          alert('ユーザ登録完了 : ' + res.user.email)\n          this.$router.push('/signin')\n        })\n        .catch(error => {\n          alert(error.message)\n        })\n    }\n  }\n}\n</script>"],"sourceRoot":"src/components"}]}